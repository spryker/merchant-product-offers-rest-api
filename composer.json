{
  "name": "spryker/product-offers-rest-api",
  "type": "library",
  "description": "MerchantProductOffersRestApi module",
  "license": "proprietary",
  "require": {
    "spryker/carts-rest-api-extension": "1.5.0",
    "spryker/kernel": "1.33.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {},
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
