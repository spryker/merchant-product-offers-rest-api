<?xml version="1.0"?>
<transfers
    xmlns="spryker:databuilder-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:databuilder-01 http://static.spryker.com/databuilder-01.xsd"
>

    <transfer name="CartItemRequest">
        <property name="sku" dataBuilderRule="isbn10"/>
        <property name="productOfferReference" dataBuilderRule="word(5)"/>
        <property name="merchantReference" dataBuilderRule="word(7)"/>
    </transfer>

    <transfer name="Item">
        <property name="sku" dataBuilderRule="isbn10"/>
        <property name="productOfferReference" dataBuilderRule="word(5)"/>
        <property name="merchantReference" dataBuilderRule="word(7)"/>
    </transfer>

</transfers>
